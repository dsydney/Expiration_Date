<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.revature.expiration_date">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.ACCESS_MEDIA_LOCATION" />
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Expiration_Date">
        <service android:name=".service.ExpiredNotification" />
        <activity
            android:name=".CameraX"
            android:exported="false"
            android:label="@string/title_activity_camera_x"
            android:theme="@style/Theme.Expiration_Date" />
        <activity
            android:name=".productview.Product"
            android:exported="false"
            android:label="@string/title_activity_product"
            android:theme="@style/Theme.Expiration_Date" />
        <activity
            android:name=".BottomNavBar"
            android:exported="false"
            android:label="@string/title_activity_bottom_nav_bar"
            android:theme="@style/Theme.Expiration_Date" />
        <activity
            android:name=".ConfirmCode"
            android:exported="false"
            android:label="@string/title_activity_confirm_code"
            android:theme="@style/Theme.Expiration_Date" />
        <activity
            android:name=".productview.ProductView"
            android:exported="false"
            android:label="@string/title_activity_product_view"
            android:theme="@style/Theme.Expiration_Date" />
        <activity
            android:name=".ProductEntry"
            android:exported="false"
            android:label="@string/title_activity_product_entry"
            android:theme="@style/Theme.Expiration_Date" />
        <activity
            android:name=".Registration"
            android:exported="false"
            android:label="@string/title_activity_registration"
            android:theme="@style/Theme.Expiration_Date" />
        <activity
            android:name=".Login"
            android:exported="false"
            android:label="@string/title_activity_main2"
            android:theme="@style/Theme.Expiration_Date" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Expiration_Date">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>